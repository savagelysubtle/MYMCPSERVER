{
  "mcpServers": {
    "personal-mcp": {
      "command": "powershell",
      "args": ["-Command", "uv run mymcpserver"],
      "env": {
        "VAULT_PATH": "${env:VAULT_PATH}",
        "LOGS_PATH": "${env:LOGS_PATH}",
        "PYTHONUNBUFFERED": "1",
        "MCP_TRANSPORT": "stdio",
        "MCP_LOG_LEVEL": "DEBUG",
        "MCP_ENABLE_RICH_LOGGING": "1"
      },
      "cwd": "${env:WORKSPACE_FOLDER}"
    }
  }
}
